<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="medical_medical_lab_list_tree_view" model="ir.ui.view">
        <field name="name">inherit.medical.lab.list.tree.view</field>
        <field name="model">medical.lab.list</field>
        <field name="inherit_id" ref="hospital_mgmt.medical_lab_list_tree_view"/>
        <field name="arch" type="xml">
           <xpath expr="//field[@name='product_id']" position = "after">
               <field name="lab_test_cost" />
            </xpath>
        </field>
    </record>
</odoo>
